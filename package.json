{
  "name": "whats-free",
  "description": "Simple page to show free rooms from your Google Apps account",
  "version": "1.0.0",
  "author": "Dan Cahoon",
  "bugs": {
    "url": "https://github.com/dpca/whats-free/issues"
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.5",
    "moment": "^2.20.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "dotenv": "^5.0.0",
    "flow-bin": "^0.66.0",
    "flow-typed": "^2.3.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "prettier": "^1.10.2",
    "react-scripts": "^1.1.1"
  },
  "homepage": "https://github.com/dpca/whats-free#readme",
  "license": "MIT",
  "lint-staged": {
    "src/**/*.js": [
      "pretty --parser flow",
      "git add"
    ],
    "*.{css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dpca/whats-free.git"
  },
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "flow": "flow",
    "flow:check": "flow check",
    "flow:install-types": "flow-typed install",
    "precommit": "lint-staged",
    "pretty": "prettier --write --single-quote --trailing-comma es5 --arrow-parens always --no-color",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom"
  }
