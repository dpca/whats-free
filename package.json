    "lodash": "^4.13.1",
    "moment": "^2.19.3",
    "prettier": "^1.8.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "dotenv": "^4.0.0",
    "flow-bin": "^0.59.0",
